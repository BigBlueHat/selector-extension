{
  "name": "selector-extension",
  "version": "0.1.0",
  "description": "Browser extension to support Selectors in URIs.",
  "scripts": {
    "build": "browserify -o extension/content_script.js src/content_script.js",
    "watch": "watchify -o extension/content_script.js src/content_script.js -v"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "dom-anchor-selector": "git+https://github.com/Treora/dom-anchor-selector.git",
    "selector-state-frags": "git+https://github.com/Treora/selector-extension.git"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
